{
  "builds": [
    {
      "src": "resume/wsgi.py",
      "use": "@vercel/python",
      "config": {"maxLambdaSize": "30mb", "runtime": "python3.10"
    },
    {
      "src": "buld.files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir":"staticfiles_build" 
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "resume/wsgi.py"
    }
  ]
}
